if ! has make ; then
	echo "make is not installed"; exit 1
fi

if ! has unzip ; then
	echo "unzip is not installed"; exit 1
fi

ROOT_DIR=$(pwd)
export ROOT_DIR

dotenv

PROVIDER_SERVICES=$CACHE_BIN/provider-services
AKASH=$CACHE_BIN/akash

export PROVIDER_SERVICES
export AKASH

make init

PATH_add $CACHE_BIN
